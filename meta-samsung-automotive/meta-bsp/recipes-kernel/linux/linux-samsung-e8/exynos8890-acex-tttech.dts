		clocks = <0x9 0x2bc 0x9 0x2bf 0x9 0x2c4 0x9 0x2c5>;
		clock-names = "aclk", "sclk", "phyclock", "pipe_pclk";
		reg = <0x0 0x15400000 0x10000>;
		#address-cells = <0x2>;
		#size-cells = <0x1>;
		ranges;
		usb-pm-qos-int = <0x3e418>;
		status = "okay";
		samsung,usb_ss_sel = <0x9f 0x1 0x1>;
		samsung,usb_board_sel = <0x68 0x2 0x1>;
		pinctrl-names = "default";
		pinctrl-0 = <0xa0 0xa1>;

		usbdrd_dwc3: dwc3 {
			compatible = "synopsys,dwc3";
			reg = <0x0 0x15400000 0x10000>;
			interrupts = <0x0 0xd5 0x0>;
			phys = <&usbdrd_phy0 0>, <&usbdrd_phy0 1>;
			phy-names = "usb2-phy", "usb3-phy";
			samsung,usb3vbus_ctrl = <0xa3 0x3 0x1>;
			samsung,boost5v_en = <0x3 0x3 0x1>;
			pinctrl-names = "default";
			pinctrl-0 = <0xa4 0xa5>;
			maximum-speed = "super-speed";
			dr_mode = "otg";
			is_not_vbus_pad;
			suspend_clk_freq = <0x3ef1480>;
		};
	};

	usb@15510000 {
		compatible = "samsung,exynos8890-dwusb2";
