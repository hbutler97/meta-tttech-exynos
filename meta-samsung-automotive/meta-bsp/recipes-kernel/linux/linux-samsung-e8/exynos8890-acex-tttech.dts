	/* Exynos PCIe port PCIEW0 */
	/* PCIe lane that connects the Exynos microprocessor and Neutrino */
	pcie0 {
		compatible = "samsung,exynos8890-pcie";
		gpios = <0x63 0x2 0x1 /* PERST */>;
		reg = <0x0 0x15660000 0x1000	/* elbi base */
			0x0 0x15630000 0x1000		/* phy base */
			0x0 0x15601044 0x10			/* sysreg base */
			0x0 0x157A0000 0x1000		/* DBI base */
			0x0 0x156F0000 0x128		/* phy pcs base */
			0x0 0x1c000000 0x1000>;		/* configuration space */
		reg-names = "elbi", "phy", "sysreg", "dbi", "pcs", "config";
		interrupts = <0x0 0xcc 0x0>; /* IRQ_PULSE */
		clocks = <0x9 0x301 0x9 0x2fa 0x9 0x2f6 0x9 0x2f7>;
		clock-names = "gate_pciewifi0", "wifi0_dig_refclk", 
				"pcie_wifi0_tx0", "pcie_wifi0_rx0";
		samsung,syscon-phandle = <0xa>;
		pinctrl-names = "default", "clkreq_output";
		pinctrl-0 = <0x64 0x65 0x66>;
		pinctrl-1 = <0x67 0x65>;
		#address-cells = <0x3>;
		#size-cells = <0x2>;
		device_type = "pci";
		ranges = <0x81000000 0x0 0x0 0x0 0x1c001000 0x0 0x00010000		 /* downstream I/O */
				0x82000000 0x0 0x1c011000 0x0 0x1c011000 0x0 0x1feefff>; /* non-prefetchable memory */
		#interrupt-cells = <0x1>;
		interrupt-map-mask = <0x0 0x0 0x0 0x0>;
		interrupt-map = <0x0 0x0 0x0 0x0 0x1 0x0 0xcc 0x4>;
		num-lanes = <0x1>;
		msi-base = <0xc8>;
		ch-num = <0x0>;
		pcie-clk-num = <0x1>;
		phy-clk-num = <0x3>;
		pcie-changed;
		pcie-pm-qos-int = <0x3e418>;
		status = "okay";
		use-msi;
	};

