	/* eMMC */
	/* Exynos uses the MSH port (supplied with 1.8V) to connect its 512GB
		eMMC NAND FLASH memory. */
	pinctrl5 {
		compatible = "samsung,exynos8890-pinctrl";
		reg = <0x0 0x10e60000 0x1000>;
		interrupts = <0x0 0xd4 0x0>;

		gpi1 {
			gpio-controller;
			#gpio-cells = <0x2>;
			interrupt-controller;
			#interrupt-cells = <0x2>;
		};

		gpi2 {
			gpio-controller;
			#gpio-cells = <0x2>;
			interrupt-controller;
			#interrupt-cells = <0x2>;
			phandle = <0xb4>;
		};

		/* EXY_EMMC_CLK */
		sd0-clk {
			samsung,pins = "gpi1-0";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			samsung,pin-drv = <0x2>;
			phandle = <0x4d>;
		};

		sd0-clk_fast_slew_rate_1x {
			samsung,pins = "gpi1-0";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			samsung,pin-drv = <0x0>;
			phandle = <0x53>;
		};

		sd0-clk_fast_slew_rate_2x {
			samsung,pins = "gpi1-0";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			samsung,pin-drv = <0x1>;
			phandle = <0x54>;
		};

		sd0-clk_fast_slew_rate_3x {
			samsung,pins = "gpi1-0";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			samsung,pin-drv = <0x2>;
			phandle = <0x55>;
		};

		sd0-clk_fast_slew_rate_4x {
			samsung,pins = "gpi1-0";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			samsung,pin-drv = <0x3>;
			phandle = <0x56>;
		};

		sd0-clk_fast_slew_rate_5x {
			samsung,pins = "gpi1-0";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			samsung,pin-drv = <0x4>;
			phandle = <0x57>;
		};

		sd0-clk_fast_slew_rate_6x {
			samsung,pins = "gpi1-0";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			samsung,pin-drv = <0x5>;
			phandle = <0x58>;
		};

		/* EXY_EMMC_CMD */
		sd0-cmd {
			samsung,pins = "gpi1-1";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x3>;
			samsung,pin-drv = <0x2>;
			phandle = <0x4f>;
		};

		/* EXY_EMMC_RDQS */
		sd0-ds {
			samsung,pins = "gpi1-2";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x1>;
			samsung,pin-drv = <0x2>;
			phandle = <0x4e>;
		};

		/* [FUNCTION NAME]: SD_0_QRDY
		/* @HBanos: This pin is grounded, but it is added anyway. */
		sd0-qrd {
			samsung,pins = "gpi1-3";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x1>;
			samsung,pin-drv = <0x2>;
		};

		/* Note: If the internal pull-ups of the MSH port of EXYNOS are not 
			enabled, the external resistors on data lanes EXY_eMMC_D0~D7
			(XMMC0DATA0~7) must be populated (R3502R3509) and R3514 
			corresponding to EXY_eMMC_RDQS (EXY pin XMMC0DS).
			By default those resistors are not populated. */
		/* EXY_EMMC_D0 */
		sd0-bus-width1 {
			samsung,pins = "gpi2-0";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x3>;
			samsung,pin-drv = <0x2>;
			phandle = <0x50>;
		};

		/* EXY_EMMC_D1 */
		/* EXY_EMMC_D2 */
		/* EXY_EMMC_D3 */
		sd0-bus-width4 {
			samsung,pins = "gpi2-1", "gpi2-2", "gpi2-3";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x3>;
			samsung,pin-drv = <0x2>;
			phandle = <0x51>;
		};

		/* EXY_EMMC_D4 */
		/* EXY_EMMC_D5 */
		/* EXY_EMMC_D6 */
		/* EXY_EMMC_D7 */
		sd0-bus-width8 {
			samsung,pins = "gpi2-4", "gpi2-5", "gpi2-6", "gpi2-7";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x3>;
			samsung,pin-drv = <0x2>;
			phandle = <0x52>;
		};
	};

	dwmmc0@15560000 {
