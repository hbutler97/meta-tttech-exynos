	/* UFS */
	/* UFS has a high speed serial interface which supports up to HS-GEAR3
		with 1Lane. (for 128Gb =>1Lane: 530 Read / 210MB/s Write) */
	/* UFS configuration */
	/* @JCastells Note: This configuration was commented in southNorth board.
					we did not test at all the functionality */
	ufs@0x155A0000 {
		compatible = "samsung,exynos-ufs";
		#address-cells = <0x2>;
		#size-cells = <0x1>;
		ranges;
		reg = <0x0 0x155a0000 0x100 0x0 0x155a0100 0x200 0x0 0x155a1000 0x800 0x0 0x155a2000 0x100>;
		interrupts = <0x0 0xd1 0x0>;
