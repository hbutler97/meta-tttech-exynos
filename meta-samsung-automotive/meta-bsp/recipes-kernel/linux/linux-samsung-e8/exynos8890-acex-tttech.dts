		hdmi_cec: hdmi_cec {
			samsung,pins = "gpa3-1";
			samsung,pin-function = <0x3>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		/* EXY_NEUT_PCIEW0_WKREQIN */
