/dts-v1/;

/memreserve/	0x00000000f0000000 0x0000000008800000;
/memreserve/	0x00000000cf000000 0x0000000001000000;

/ {
	compatible = "samsung,exynos8890"; // Note: "samsung,AUDI8890" removed.
	interrupt-parent = <0x1>;
	//#address-cells = <0x2>;
	//#size-cells = <0x1>;
	model = "Samsung ACEX Base board rev X.X based on EXYNOS8890";

	/* Copied over exynos8890-at3-tttech.dts */
	reserved-memory {
		#address-cells = <0x2>;
		#size-cells = <0x1>;
		ranges;

		crypto {
			compatible = "exynos8890-ion,crypto";
			reusable;
			reg = <0x0 0xc0000000 0x800000>;
			id = <0x1>;
		};

		video_fw {
			compatible = "exynos8890-ion,vfw";
			secure;
			reusable;
			noprot;
			reg = <0x0 0xc0800000 0x400000>;
			id = <0x2>;
			compat-id = <0x18>;
		};

		video_nfw {
			compatible = "exynos8890-ion,vnfw";
			reusable;
			reg = <0x0 0xc0c00000 0x400000>;
			id = <0x7>;
			compat-id = <0x14>;
		};

		video_stream {
			compatible = "exynos8890-ion,vstream";
			secure;
			reusable;
			reg = <0x0 0xc1001000 0xfff000>;
			id = <0x3>;
			compat-id = <0x19 0x1f>;
		};

		camera {
			compatible = "exynos8890-ion,camera";
			reg = <0x0 0xc1000000 0x1000>;
			id = <0xa>;
		};

		video_frame {
			compatible = "exynos8890-ion,vframe";
			reg = <0x0 0xc2000000 0x4000000>;
			id = <0x5>;
			compat-id = <0x1a 0x1d>;
		};

		video_scaler {
			compatible = "exynos8890-ion,vscaler";
			secure;
			reusable;
			reg = <0x0 0xc6000000 0x3c00000>;
			id = <0x6>;
			compat-id = <0x1c>;
		};

		gpu_crc {
			compatible = "exynos8890-ion,gpu_crc";
			secure;
			reg = <0x0 0xc9c00000 0x800000>;
			id = <0x8>;
		};

		gpu_buffer {
			compatible = "exynos8890-ion,gpu_buffer";
			secure;
			reusable;
			reg = <0x0 0xca400000 0x2400000>;
			id = <0x9>;
		};

		sec_area_reserved {
			reg = <0x0 0xfe400000 0x1c00000>;
		};
	};

	/* @JCastells: Is this actually a vald set of bootargs now? */
	/* @JCastells: TBC if console=ttySAC2 */
	chosen {
		bootargs = "console=ttySAC2,115200 root=/dev/mmcblk0p2 rootwait 
			consoleblank=0 loglevel=1 ess_setup=0x86000000 pmic_info=3 
			rootfstype=ext4 init=/sbin/early-boot";
	};

	/* Copied over at3 dts */
	/* Exynos Clock 26MHz */
	fixed-rate-clocks {
		oscclk {
			compatible = "samsung,exynos8890-oscclk";
			clock-frequency = <26000000>;
		};
	};

	/* GPIO setup */
	pinctrl8 {
		/* EXY_NEUT_RESETn - Neutrino Reset */
		neut_reset: neut_reset {
			samsung,pins = "gpb1-0";
			samsung,pin-function = <0x1>;
			samsung,pin-val = <0x1>;
		};

		/* AUR_EXY_STP_MODE_1 - Debugging purpose */
		stp_mode_1: stp_mode_1 {
			samsung,pins = "gpb1-1";
			samsung,pin-function = <0x0>;
		};

		vbus_ctrl: vbus_ctrl {
			samsung,pins = "gpi3-6";
			samsung,pin-function = <1>;
			samsung,pin-val = <0>;
		};
	};

	pinctrl@14CA0000 {
		/* AUR_EXY_STP_MODE_2 - Debugging purpose */
		stp_mode_2: stp_mode_2 {
			samsung,pins = "gpf3-4";
			samsung,pin-function = <0x0>;
		};
	};

	pinctrl7 {
		compatible = "samsung,exynos8890-pinctrl";
		reg = <0x0 0x14cd0000 0x1000>;
		interrupts = <0x0 0x1c8 0x0>;

		gpf0 {
			gpio-controller;
			#gpio-cells = <0x2>;
			interrupt-controller;
			#interrupt-cells = <0x2>;
			phandle = <0x35>;
		};

		/* EXY_NEUT_INTO - Neutrino interrupt OUT to Exynos */
		neut_into: neut_into {
			samsung,pins = "gpf0-2";
			samsung,pin-function = <0>;
		};
	};

	pinctrl@14CE0000 {
		compatible = "samsung,exynos8890-pinctrl";
		reg = <0x0 0x14ce0000 0x1000>;
		interrupts = <0x0 0x1c9 0x0>;

		gpf1 {
			gpio-controller;
			#gpio-cells = <0x2>;
			interrupt-controller;
			#interrupt-cells = <0x2>;
			phandle = <0x37>;
		};

		/* EXY_NEUT_INTI - Neutrino Interrupt IN from Exynos */
		neut_inti: neut_inti {
			samsung,pins = "gpf1-2";
			samsung,pin-function = <1>;
			//samsung,pin-val = <0>;
		};

		/* I2C 7 */
		hs-i2c7-bus {
			samsung,pins = "gpf1-1", "gpf1-0";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			samsung,pin-drv = <0x0>;
			phandle = <0x36>;
		};
	};


	/* PERIC1 */
	pinctrl@14CC0000 {
		compatible = "samsung,exynos8890-pinctrl";
		reg = <0x0 0x14cc0000 0x1000>;
		interrupts = <0x0 0x1cc 0x0>;

		gpe0 {
			gpio-controller;
			#gpio-cells = <0x2>;
			interrupt-controller;
			#interrupt-cells = <0x2>;
			phandle = <0x7e>;
		};

		gpe5 {
			gpio-controller;
			#gpio-cells = <0x2>;
			interrupt-controller;
			#interrupt-cells = <0x2>;
			phandle = <0x2e>;
		};

		gpe6 {
			gpio-controller;
			#gpio-cells = <0x2>;
			interrupt-controller;
			#interrupt-cells = <0x2>;
			phandle = <0x1c>;
		};

		gpj1 {
			gpio-controller;
			#gpio-cells = <0x2>;
			interrupt-controller;
			#interrupt-cells = <0x2>;
			phandle = <0x63>;
		};

		gpj2 {
			gpio-controller;
			#gpio-cells = <0x2>;
			interrupt-controller;
			#interrupt-cells = <0x2>;
		};

		gpe2 {
			gpio-controller;
			#gpio-cells = <0x2>;
			interrupt-controller;
			#interrupt-cells = <0x2>;
			phandle = <0x1e>;
		};

		gpe3 {
			gpio-controller;
			#gpio-cells = <0x2>;
			interrupt-controller;
			#interrupt-cells = <0x2>;
		};

		gpe4 {
			gpio-controller;
			#gpio-cells = <0x2>;
			interrupt-controller;
			#interrupt-cells = <0x2>;
			phandle = <0x3>;
		};

		gpe1 {
			gpio-controller;
			#gpio-cells = <0x2>;
			interrupt-controller;
			#interrupt-cells = <0x2>;
			phandle = <0x33>;
		};

		gpe7 {
			gpio-controller;
			#gpio-cells = <0x2>;
			interrupt-controller;
			#interrupt-cells = <0x2>;
			phandle = <0x68>;
		};

		gpg0 {
			gpio-controller;
			#gpio-cells = <0x2>;
			interrupt-controller;
			#interrupt-cells = <0x2>;
		};


		/* EXY_FPDL_PDB - HDMI to FPDLink III serializer enable signal */
		fpdl_pdb: fpdl_pdb {
			samsung,pins = "gpe6-4";
			samsung,pin-function = <1>;
			samsung,pin-val = <0>;
		};

		/* EXY_FPDL_REM_INTB - FPDL Remote interrupt. Mirrors status of EXY_FPDL_INTB */
		fpdl_rem_intb: fpdl_rem_intb {
			samsung,pins = "gpe6-5";
			samsung,pin-function = <0>;
			samsung,pin-val = <0>;
		};

		/* EXY_FPDL_INTB - Remote interrupt from FPDL */
		fpdl_intb: fpdl_intb {
			samsung,pins = "gpe6-6";
			samsung,pin-function = <0>;
			samsung,pin-val = <0>;
		};

		/* EXY_EMMC_NRESET - eMMC reset pin */
		emmc_nreset: emmc_nreset {
			samsung,pins = "gpg0-2";
			samsung,pin-function = <1>;
			samsung,pin-val = <0>;
		};

		/* EXY_USB_GRSTz - reset for USB HUB */
		usb_grst: usb_grst {
			samsung,pins = "gpj1-3";
			samsung,pin-function = <1>;
			samsung,pin-val = <0>;
		};

		/* EXY_GMSL_GPIO0 - Open drain GPIO (internal 60K pull-up) */
		gmsl_gpio0: gmsl_gpio0 {
			samsung,pins = "gpe4-4";
			//samsung,pin-function = <0x0>;
			samsung,pin-pud = <0x3>; // 0x3 - enables pull-up
			samsung,pin-drv = <0>;
		};

		/* EXY_GMSL_GPIO1 - Open drain GPIO (internal 60K pull-up) */
		gmsl_gpio1: gmsl_gpio1 {
			samsung,pins = "gpe4-5";
			//samsung,pin-function = <0x0>;
			samsung,pin-pud = <0x3>; // 0x3 - enables pull-up
			samsung,pin-drv = <0>;
		};

		/* EXY_GMSL_CCBSY - channel busy output (internal 60k pull-up) */
		gmsl_ccbsy: gmsl_ccbsy {
			samsung,pins = "gpe4-6";
			samsung,pin-function = <0x0>;
			samsung,pin-pud = <0x3>;
			samsung,pin-drv = <0>;
		};

		/* EXY_GMSL_ERR - error output (internal 60k pull-up) */
		gmsl_err: gmsl_err {
			samsung,pins = "gpe4-7";
			samsung,pin-function = <0x0>;
			samsung,pin-pud = <0x3>;
			samsung,pin-drv = <0>;
		};

		/* EXY_GMSL_LFLT - line fault output (internal 60k pull-up) */
		gmsl_lflt: gmsl_lflt {
			samsung,pins = "gpe4-0";
			samsung,pin-function = <0x0>;
			samsung,pin-pud = <0x3>;
			samsung,pin-drv = <0>;
		};

		/* EXY_GMSL_LMO - line memory overflow output (internal 60k pull-up)*/
		gmsl_lmo: gmsl_lmo {
			samsung,pins = "gpe4-1";
			samsung,pin-function = <0x0>;
			samsung,pin-pud = <0x3>;
			samsung,pin-drv = <0>;
		};

		/* PLL - Lock output */
		gmsl_lock: gmsl_lock  {
			samsung,pins = "gpe4-2";
			samsung,pin-function = <0x0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;

		};

		/* EXY_GMSL_PWDN - power-down input of the GMSL deserializer */
		gmsl_pwdn: gmsl_pwdn {
			samsung,pins = "gpe4-3";
			samsung,pin-function = <0x1>;
			samsung,pin-pud = <1>;
			samsung,pin-val= <1>;
		};

		/* EXY_XUSB30VBUS0 - Indicate supply of USB device */
		xusb30vbus0: xusb30vbus0 {
			samsung,pins = "gpe3-4";
			samsung,pin-function = <0x0>;
		};

		/* EXY_XUSB30OVERCUR - Signalize overcurrent on USB */
		xusb30overcur: xusb30overcur {
			samsung,pins = "gpe3-5";
			samsung,pin-function = <0x0>;
		};

		/* EXY_XUSB30VBUS_CTRL - Enable voltage supply for USBs */
		xusb30vbus_ctrl: xusb30vbus_ctrl {
			samsung,pins = "gpe3-6";
			samsung,pin-function = <0x1>;
            /* Set the EXY_XUSB30VBUS_CTRL to high to enable voltages 
                for the USB */
            samsung,pin-val = <1>;
		};

		/* EXY_UHOST_CUR - Enables 2nd stage of current (1.2Amp) for USB */
		uhost_curr: uhost_curr {
			samsung,pins = "gpe3-7";
			samsung,pin-function = <1>;
			samsung,pin-val = <0>;
		};

		/* +8V0_EXY_PoC_EN */
		/* Enables Power over Coax supply for Cameras */
		exy_poc_en: exy_poc_en {
			samsung,pins = "gpe2-4";
			samsung,pin-function = <1>;
		};

		/* PMIC */
		/* PG_1V1DCDC_EXY */
		/* Monitor stages of the PMIC (last enabled) */
		pg_1v1dcdc_exy: pg_1v1dcdc_exy {
			samsung,pins = "gpe2-5";
			samsung,pin-function = <0x0>;
		};

		/* I2C 2 */
		hs-i2c2-bus {
			samsung,pins = "gpe5-1", "gpe5-0";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			samsung,pin-drv = <0x0>;
			phandle = <0x2d>;
		};

		/* I2C 3 */
		hs-i2c3-bus {
			samsung,pins = "gpe5-3", "gpe5-2";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			samsung,pin-drv = <0x0>;
			phandle = <0x2f>;
		};

		/* I2C 12 */
		hs-i2c12-bus {
			samsung,pins = "gpe5-7", "gpe5-6";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			samsung,pin-drv = <0x0>;
			phandle = <0x42>;
		};

		/* I2C 13 */		
		hs-i2c13-bus {
			samsung,pins = "gpe6-1", "gpe6-0";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			samsung,pin-drv = <0x0>;
			phandle = <0x43>;
		};

		/* I2C 14 */
		hs-i2c14-bus {
			samsung,pins = "gpe6-3", "gpe6-2";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			samsung,pin-drv = <0x0>;
			phandle = <0x44>;
		};

		/* I2C 8 */
		/* I2C communication HDMI-FPD-Link III Serializer */
		/* EXY_FPDL_I2C_SCL */
		/* EXY_FPDL_I2C_SDA */
		hs-i2c8-bus {
			samsung,pins = "gpe5-5", "gpe5-4";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			samsung,pin-drv = <0x0>;
			phandle = <0x3b>;
		};

		/* PCIe0 */
		/* EXY_NEUT_PCIEW0_CLKREQ - NEW from SIP */
		pcie0_clkreq {
			samsung,pins = "gpj1-0";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x3>;
			samsung,pin-drv = <0x3>;
			samsung,pin-con-pdn = <0x3>;
			samsung,pin-pud-pdn = <0x3>;
			phandle = <0x64>;
		};

		pcie0_clkreq_output {
			samsung,pins = "gpj1-0";
			samsung,pin-function = <0x1>;
			samsung,pin-pud = <0x3>;
			samsung,pin-drv = <0x3>;
			samsung,pin-con-pdn = <0x3>;
			samsung,pin-pud-pdn = <0x3>;
			samsung,pin-val = <0x1>;
			phandle = <0x67>;
		};

		/* EXY_NEUT_PCIEW0_PRESET - NEW from SIP */
		pcie0_perst {
			samsung,pins = "gpj1-2";
			samsung,pin-function = <0x1>;
			samsung,pin-drv = <0x3>;
			samsung,pin-con-pdn = <0x3>;
			phandle = <0x65>;
		};

		/* PCIe1 */
		/* EXY_CONN_PCIEW1_CLKREQ - NEW from SIP */
		pcie1_clkreq {
			samsung,pins = "gpj1-4";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x3>;
			samsung,pin-drv = <0x3>;
			samsung,pin-con-pdn = <0x3>;
			samsung,pin-pud-pdn = <0x3>;
			phandle = <0x69>;
		};

		pcie1_clkreq_output {
			samsung,pins = "gpj1-4";
			samsung,pin-function = <0x1>;
			samsung,pin-pud = <0x3>;
			samsung,pin-drv = <0x3>;
			samsung,pin-con-pdn = <0x3>;
			samsung,pin-pud-pdn = <0x3>;
			samsung,pin-val = <0x1>;
			phandle = <0x6b>;
		};

		/* EXY_CONN_PCIEW1_PRESET - NEW from SIP */
		pcie1_perst {
			samsung,pins = "gpj1-6";
			samsung,pin-function = <0x1>;
			samsung,pin-drv = <0x3>;
			samsung,pin-con-pdn = <0x3>;
			phandle = <0x6a>;
		};

		/* EXY_CONN_PCIEW1_PWRON - PCIeW1 reset */
		conn_pciew1_pwron: conn_pciew1_pwron {
			samsung,pins = "gpj1-7";
			samsung,pin-function = <1>;
			samsung,pin-val = <0>;
			samsung,pin-pud = <0x0>;
			samsung,pin-con-pdn = <0x3>;
			linux,phandle = <0xab>;
			phandle = <0xab>;
		};

		/* SPI Interface with NEUTRINO */
		/* NOTE: config different from SIP - gpe2-1 (CS) is separated */
		spi0-bus {
			samsung,pins = "gpe2-3", "gpe2-2", "gpe2-1", "gpe2-0";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x3>;
			samsung,pin-drv = <0x0>;
			phandle = <0x16>;
		};

		/* EXY_PCIe_CTRL_EN */
		/* Enables Out of the level shifter for PCIe1 */
		pcie1_ctrl_en: pcie1_ctrl_en {
			samsung,pins = "gpe2-6";
			sam;sung,pin-function = <0x1>;
		};

		/* EXY_NEUT_UART_RXD */
		/* EXY_NEUT_UART_TXD */
		uart1-bus {
			samsung,pins = "gpe0-3", "gpe0-2", "gpe0-1", "gpe0-0";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			phandle = <0x11>;
		};

		/* EXY_AP_UART_RXD */
		/* EXY_AP_UART_TXD */
		uart2-bus {
			phandle = <0x12>;
		};

		/* NOT USED */
		uart3-bus {
			samsung,pins = "gpe0-7", "gpe0-6";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			phandle = <0x13>;
		};


		/* EXY_UART_RXD_4 */
		/* EXY_UART_TXD_4 */
		uart4-bus {
			samsung,pins = "gpe1-3", "gpe1-2", "gpe1-1", "gpe1-0";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			phandle = <0x14>;
		};

		/* EXY_UART_RXD_5 */
		/* EXY_UART_TXD_5 */
		uart5-bus {
			samsung,pins = "gpe1-7", "gpe1-6", "gpe1-5", "gpe1-4";
			samsung,pin-function = <0x2>;
			samsung,pin-pud = <0x0>;
			phandle = <0x15>;
		};
	};

	/* eMMC 5.0 configuration */
	/* Not touched. Pinout is the same as the one in SouthNorth */
	/* What to do with EXY_EMMC_RESET?? */
	/*SN: KLMCG8GESD-B03P  ACEX: KLMCG8GESD-B04Q */
	/*Are different devices, but quite similar. Maybe we'll need to tweak one 
	  or two options?
	*/

	/* GMSL IN (cameras) TODO TODO TODO */
	/* Pending. Nothing done here*/


	hsi2c@10550000 {
		compatible = "samsung,exynos5-hsi2c";
		samsung,check-transdone-int;
		reg = <0x0 0x10550000 0x2000 0x0 0x10540180 0x100>;
		interrupts = <0x0 0x154 0x0 0x0 0x155 0x0>;
		#address-cells = <0x1>;
		#size-cells = <0x0>;
		pinctrl-names = "default";
		pinctrl-0 = <0x45>;
		clocks = <0x9 0x3b6 0x9 0x3b6>;
		clock-names = "rate_hsi2c", "gate_hsi2c";
		samsung,scl-clk-stretching;
		gpio_sda = <0x46 0x0 0x1>;
		gpio_scl = <0x46 0x1 0x1>;
		status = "okay";
		samsung,hs-mode;
		clock-frequency = <0x2dc6c0>;
		samsung,hsi2c-batcher;
		samsung,use-apm;
		samsung,apm-always-clkon;

		s2mps16_pmic@66 {
			compatible = "samsung,s2mps16-pmic";
			reg = <0x66>;
			interrupts = <0x2 0x0 0x0>;
